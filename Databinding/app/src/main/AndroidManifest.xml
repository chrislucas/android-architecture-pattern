        </activity>
        <activity android:name=".activities.CallPhone" />
        <activity android:name=".activities.HandlerEventUsingDatabinding">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name=".broadcast.FilterAll"
            android:enabled="true"
            android:exported="true">
